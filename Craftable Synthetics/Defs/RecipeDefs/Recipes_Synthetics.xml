<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

	<RecipeDef>
		<defName>CreateSynthread</defName>
		<label>Create Synthread</label>
		<description>Create Synthread from Devilstrand, Cloth and Steel.</description>
		<researchPrerequisite>SynthreadTechCrafting</researchPrerequisite>
		<jobString></jobString>
		<workAmount>1300</workAmount>
		<workSpeedStat>TailoringSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.75</workSkillLearnFactor>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<targetCountAdjustment>1</targetCountAdjustment>
		<recipeUsers>
			<li>ElectricTailoringBench</li>
			<li>HandTailoringBench</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>DevilstrandCloth</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>35</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>DevilstrandCloth</li>
				<li>Cloth</li>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Synthread>75</Synthread>
		</products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>6</minLevel>
			</li>
		</skillRequirements>
	</RecipeDef>

	<RecipeDef>
		<defName>CreateHyperweave</defName>
		<label>Create Hyperweave</label>
		<description>Create Hyperweave from Synthread and Plasteel</description>
		<researchPrerequisite>HyperweaveTechCrafting</researchPrerequisite>
		<jobString></jobString>
		<workAmount>1900</workAmount>
		<workSpeedStat>TailoringSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>1.00</workSkillLearnFactor>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<targetCountAdjustment>1</targetCountAdjustment>
		<recipeUsers>
			<li>ElectricTailoringBench</li>
			<li>HandTailoringBench</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Synthread</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>35</count>
			</li>	
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Synthread</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Hyperweave>75</Hyperweave>
		</products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>12</minLevel>
			</li>
		</skillRequirements>
	</RecipeDef>



</RecipeDefs>